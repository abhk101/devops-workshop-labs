#!/bin/bash

echo "------ START BUILD INFO ------- "
echo $BUILD
echo $APP_VERSION
env
echo "------ END BUILD INFO ------------"

echo "Before assembling"

/usr/libexec/s2i/assemble
rc=$?

if [ $rc -eq 0 ]; then
    echo "After successful assembling"
}'

else
    echo "After failed assembling"
}'

fi

exit $rc
